<Application x:Class="VP_Mobile.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VP_Mobile"
             Startup="Application_Startup"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="VPTeal" Color="#00b3b3"/>
        <SolidColorBrush x:Key="VPOrange" Color="Orange"/>
        <SolidColorBrush x:Key="VPGray" Color="LightGray"/>
        <SolidColorBrush x:Key="VPUnGeocoded" Color="Pink"/>
        <SolidColorBrush x:Key="VPAssigned" Color="DarkRed"/>
    </Application.Resources>
</Application>
